{
  "shortcuts": {
    "returnToMenu": {
      "ctrl": true,
      "key": "e"
    },
    "cancel": {
      "key": "escape"
    }
  },
  "statusHooks": {},
  "worktreeHooks": {
    "post_creation": {
      "command": "cd $CCMANAGER_WORKTREE_PATH && ruler apply --agents=claude,cursor",
      "enabled": true
    }
  },
  "worktree": {
    "autoDirectory": true,
    "autoDirectoryPattern": "../{project}-workspaces/{branch}",
    "copySessionData": true
  },
  "command": {
    "command": "claude"
  },
  "commandPresets": {
    "presets": [
      {
        "id": "1",
        "name": "Claude",
        "command": "claude"
      },
      {
        "id": "2",
        "name": "Claude + Resume",
        "command": "claude",
        "args": ["--resume"]
      },
      {
        "id": "3",
        "name": "Claude + Yolo",
        "command": "claude",
        "args": ["--dangerously-skip-permissions"]
      },
      {
        "id": "4",
        "name": "Claude + Cursor",
        "command": "cursor-tunnel-launch.sh"
      },
      {
        "id": "5",
        "name": "Claude + Cursor + Resume",
        "command": "cursor-tunnel-launch.sh",
        "args": ["--resume"]
      },
      {
        "id": "6",
        "name": "Claude + Cursor + Yolo",
        "command": "cursor-tunnel-launch.sh",
        "args": ["--dangerously-skip-permissions"]
      }
    ],
    "defaultPresetId": "1",
    "selectPresetOnStart": true
  }
}
